{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/admin/*": ["admin/*"],
      "@/components/*": ["src/components/*"],
      "@/layout/*": ["src/layout/*"],
      "@/utils/*": ["src/utils/*"],
      "@/sections/*": ["src/sections/*"],
      "@/public/*": ["public/*"],
      "@/src/*": ["src/*"],
      "@/*": ["./*"]
    },
    "resolveJsonModule": true
  },
  "include": ["**/*.js", "**/*.jsx"],
  "exclude": ["node_modules"]
}
